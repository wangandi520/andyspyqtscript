    def getStockCodeFromTxt(self):
        # 读取股票代码文件stock.txt
        readFileData = []
        with open('stock.txt', mode='r', encoding='UTF-8') as file:
            readFileData = file.readlines()
        newFileData = []
        for eachData in readFileData:
            newFileData.append(eachData.replace('\n',''))
        return newFileData

