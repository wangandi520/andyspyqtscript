    def getRedOrGreen(self, nowPrice, yesterdayPrice):
        # 判断价格颜色，使用当前价格和昨收价格比对
        if float(nowPrice) > float(yesterdayPrice):
            return 'red'
        elif float(nowPrice) == float(yesterdayPrice):
            return 'black'
        elif float(nowPrice) < float(yesterdayPrice):
            return 'green'
        
